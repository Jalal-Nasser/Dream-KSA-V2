{
  "expo": {
    "name": "Dreams KSA",
    "slug": "dream-ksa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dream-ksa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.dreamksa",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "app.dreamksa",
      "permissions": [
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "BLUETOOTH_CONNECT"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [ { "scheme": "dream-ksa", "host": "auth", "pathPrefix": "/callback" } ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8d71da18-3c79-489a-aefa-e3cc55839bb3"
      },
      "EXPO_PUBLIC_HMS_LISTENER_ROLE": "viewer",
      "EXPO_PUBLIC_HMS_SPEAKER_ROLE": "host",
      "EXPO_PUBLIC_SUPABASE_URL": "https://kgcpeoidouajwytndtqi.supabase.co",
      "EXPO_PUBLIC_SUPABASE_ANON": "",
      "EXPO_PUBLIC_HMS_TOKEN_URL": "https://kgcpeoidouajwytndtqi.supabase.co/functions/v1/hms-token"
    },
    "owner": "jnasser"
    ,
    "jsEngine": "jsc"
  }
}
